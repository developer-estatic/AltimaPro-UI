                <script>
											function handleClick(element, id) {
												// Call the existing functionality
                        quickeditcode(element, id, 'quick_list', 'quick_status', 'check_button', 'quick_reload');
												
												// Hide the pencil icon
												var pencilIcon = document.querySelector('#quick_drop' + id);
												if (pencilIcon) {
													pencilIcon.style.display = 'none';
												}
											}

											$(document).ready(function() {
												// Add event listener to the select element
												$('.confmodal14').click(function() {
													// Get the initial selected value from the paragraph
													var initialValue = $(this).closest(".voipmanagment-status-dropdwn").find("p").text().trim();
													var initialSelectedValue = (initialValue === "Active") ? "1" : "0";
													// alert(initialSelectedValue);

													// Get the currently selected value of the dropdown
													var selectedValue = $(this).val();
													// alert(selectedValue);

													// Compare the initial and current values
													if (selectedValue !== initialSelectedValue) {
														$('#changestatus14').modal('show');
													}
												});
											});
										</script>

                    
