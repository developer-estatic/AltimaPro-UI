<script>
var modalOpendId;
$(document).ready(function(){  
	 var lang = 'en';
   var table = $('#DtBunit').DataTable({
responsive  : true,
	ordering    : true,
    seraching:false,
	// columnDefs: [
    //    { orderable: "false",
	// 	 type: "date-uk",
	// 	 targets: "sort-date",
	// 	}
	//  ],
	//  "paging": true,
	//  "pageLength": 20,
    //  "lengthMenu": [[20, 40, 60, -1], [20, 40, 60, "All"]],
    bLengthChange: false,
     dom: 'lrtip',
     buttons: [],
  rowCallback: function (
                row,
                data,
                displayNum,
                displayIndex,
                dataIndex
            ) {
				$(row).mouseover(function () {
                    showBtns($(this));
                });
			},
      drawCallback: function (settings) {
          $("#DtBunit_paginate").detach().appendTo("#pagination_dummy_div");
          $("#DtBunit_info").detach().appendTo("#pagination_dummy_div2");
          $("#length_change").val(this.api().page.info().length);
      },
	 });
  //  $(document).on("click", "#edit_dt", function () {
  //       var dataid = $(this).attr("data-id");
  //       var dataaction = $(this).attr("data-action");
  //       // $("#"+dataid+" .modal-body").load(dataaction);
  //       // $("#"+dataid).modal("show");
  //       // ALTCRM-223
  //       $.ajax({
  //         type: "POST",
  //         url: dataaction,
  //         success: function (html) {
  //           $("#"+dataid+" .modal-body").html(html);
  //           $("#"+dataid).modal('show');
  //         },
  //       });
  //       // end ALTCRM-223
  //   });
    $(document).on("click", ".edit_dt", function() {
      var dataid = $(this).attr("data-id");
      var dataaction = $(this).attr("data-action");
      // $("#"+dataid+" .modal-body").load(dataaction);
      // $("#"+dataid).modal("show");
      // ALTCRM-223
      $.ajax({
        type: "POST",
        url: dataaction,
        success: function(html) {
          $("#" + dataid + " .modal-body").html(html);
          $("#" + dataid).modal('show');
        },
      });
      // end ALTCRM-223
    });

    $("#custsearch").on("keyup", function () {
            table.search(this.value).draw();
        });
        
  });



  function showBtns(row) {
      $(row).css("position", "relative");
      var dlt_btn = $("#leads_btns").find("button#delete_dt");
      // var dlt_btn = $("#chkbdel").find("a#delete_dt");
      var edt_btn = $("#leads_btns").find("button#edit_dt");
      dlt_btn.attr("data-target", "");
      edt_btn.attr("data-target", "");
      $("#leads_btns").css({
          "z-index": "100",
          display: "block",
          position: "absolute",
          top: parseInt($(row).offset().top-55) + "px",
      });
      var dataid = row
          .find("td:eq(0)")
          .find("a")
          .data("id");
      var url = base_url+ lang + "/admin/loda_data_worldcheck_edit/"+dataid;
    var deletemodal = "#del"+dataid;
      dlt_btn.attr("data-target", deletemodal);
      edt_btn.attr("data-action", url);
      edt_btn.attr("data-id", dataid);

      // console.log(dataid);
  }

  $(".lead-search button").click(function(){
     $(".lead-search .form-control").toggleClass("input-collapsed2");
  });

  // ALTCRM-1422
  $(function() {
    $("#flowcheckall").click(function() {
      // alert("hello");

      $(".chk_boxes1").prop("checked", this.checked);
    });
  });

  $('#tooltipdelete a').tooltip({
    title: "Delete",
    placement: "top",
    container: 'body',
    template: '<div class="tooltip" role="tooltip"><div class="tooltip-arrow"></div><div class="tooltip-inner"></div></div>',
  });

  function handleClick1(e) {
    if ($(e).is(":checked")) {
      checkboxvalue_array = $('[name="check[]"]:checked').length;
      $(e).parent().parent().parent().addClass("active-account");
    } else {
      checkboxvalue_array = $('[name="check[]"]:checked').length;
      $(e).parent().parent().parent().removeClass("active-account");
    }
    checkedoptOperation(checkboxvalue_array);
  }

  var checkboxvalue_array;
  function handleAllcheckedoption(e) {
    if ($(e).is(":checked")) {
      checkboxvalue_array = $('[name="check[]"]:checked').length;
      checkedoptOperation("All");

    } else {
      checkboxvalue_array = $('[name="check[]"]:checked').length;
      checkedoptOperation(0);
    }
  }

  checkedoptOperation();
  function openStatusmodal(e) {
      var returnvar = true;
      var lengthcount = $('[name="check[]"]:checked').length;
      // alert(lengthcount);
      
      if ($('input.chk_boxes1:checked').length === 0) {
          returnvar = false;
          $(".alertpopup").show();
      } else {
          $('#nofrecords').html($('input.chk_boxes1:checked').length)
          
          $("#assign_modal1").modal("show");
      }
  }
  $("#submitassignedbtnall").click(function () {
      var returnvar = true;
      var lengthcount = $('[name="check[]"]:checked').length;
      if ($("input[class=chk_boxes1]:checked").length == 0) {
          returnvar = false;
          $(".alertpopup").show();
      } else {
          var checkedlead = $('input[name="check[]"]');
          var checkedleadselected = [];
          $('[name="check[]"]:checked').each(function (checkedlead) {
              checkedleadselected.push($(this).val());
          });
          $("#arrayofcheckedlead").val(checkedleadselected);
          console.log(checkedleadselected);

          $.ajax({
            type: "POST",
            url: base_url + lang + "/admin/delete_worlcheck/",
            data: {
                "checkedleadselected": checkedleadselected,
            },
            success: function(html) {
                // alert(html);
                window.location.reload();

            },
        });
          
      }

      return returnvar;
  });

  function checkedoptOperation(count) {
    if (count > 0 || count === "All") {
      if(count === 'All'){
        count ='All' ;
      }
      $("#list-view-control-id").hide();
      $("#checked-opt-opearation").show();
      $(".selectedcheckbox").text(`Selected ${count} WorldCheck Details${count === 1 ? '' :''}`);
    } else {
      $("#checked-opt-opearation").hide();
      $("#list-view-control-id").show();
    }
  }

  $('.changeWorldCheckModal').on('hidden.bs.modal', function () {
  $(`#quick_list${modalOpendId}`).hide()
  $(`#quick_status${modalOpendId}`).show()
  $(`#quick_drop${modalOpendId}`).show()
})
</script></div>

